<%layout("./layout/boilerplate.ejs")%>
<h1 class="offset-2 mt-5">Sign Up form</h1>
<div class="row">
    <div class="col-6 needs-validation offset-2" novalidate>
        <form method="post" action="/signupp" class="needs-validation" novalidate>
        <div class="mb-3">
        <label 
        for="Username" >
      Username
    </label>
        <input name="Username" id="Username"  required class="form-control">
        <div class="invalid-feedback">Username must be enter</div>
        <div class="valid-feedback">Look's Good..!</div>
        </div>
        <div class="mb-3">
        <label for="email" >Email</label>
        <input name="email"
        type="email"
         required class="form-control">
         <div class="invalid-feedback">Email must be enter</div>
        <div class="valid-feedback">Look's Good..!</div>
        </div>
        <div class="mb-3">
        <label for="pass" >Password</label>
        <input name="password" required type="password" id="pass" class="form-control">
        <div id="passwordHelpBlock" class="form-text">
  Your password must be 8-20 characters long, contain letters and numbers, and must not contain spaces, special characters, or emoji.
</div>
        <div class="invalid-feedback">password must be enter</div>
        <div class="valid-feedback">Look's Good..!</div>
        </div>
            <button class="btn btn-success">Sign Up</button>
        </form>
    </div>
</div>